#include "picmt-msm8916-camera-sensor-qrd-skui.dtsi"
#include "dsi-panel-trulyotm1288a-720p-video.dtsi"
#include "dsi-panel-trulyotm1288a-720p-cmd.dtsi"
#include "dsi-panel-yushunNT35520-720p-cmd.dtsi"
